<% @events.each do |event| %>
      <ul>
    <li> 
      Title: <%= link_to event.title, event %> <br> 
      Date: <%= event.date %> <br>
      Time: <%= event.time %> <br>
      At: <%= event.location %> <br>
      Description: <%= event.description %> <br>
      Deadline: <%= event.deadline %> <br>
      Website: <a href='http://<%= event.url %>'> <%= event.url %> </a>
    </li>
  </ul>
<div class="card">
    <div class="cardTitle"><strong><%= link_to event.title, event %></strong></div>
    <div class="cardRest col-sm-12 col-xs-12 col-md-12 col-lg-12">
        <div class="cardElem col-xs-7 col-sm-7 col-md-7 col-lg-7" style="padding: 0px">Date: <%= event.date %></div>
        <div class="cardElem col-xs-5 col-sm-5 col-md-5 col-lg-5" style="padding: 0px;">Time: <%= event.time %></div>
        <div class="clearfix"></div>
        <div class="cardElem col-xs-6 col-sm-6 col-md-6 col-lg-6" style="padding-left: 0px; padding-top: 5px">Description: <p><%= event.description %></p></div>
        <div style="padding: 0px" class="cardCal col-md-5 col-sm-5 col-xs-5 col-lg-5">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 cardCalMonth">
                Month
                </div>
                00
        </div>

    </div>

</div>
<br>
<% end %>
